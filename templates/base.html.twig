<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    <link rel="icon"
          href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
    {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
        <link rel="stylesheet" href="{{ asset('css/normalize.css') }}">
        <link rel="stylesheet" href="{{ asset('css/skeleton.css') }}">
        <link rel="stylesheet" href="{{ asset('css/styles.css') }}">
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
</head>
<body>
<div class="row">
    <div class="three columns ">
        <div class="identification margin-bottom"> {% block identification %} Identification {% endblock %} </div>
        <div class="service margin-bottom"> {% block service %} Catégorie de service {% endblock %} </div>
    </div>
    <div class="nine columns ">
        <div class="bandeau margin-bottom">
        {% block bandeau %} Bandeau {% endblock %}
        </div>
        <main>
            <div class="recherche margin-bottom">{% block recherche %} recherche {% endblock %}</div>
            <div class="main margin-bottom">{% block body %} MAIN {% endblock %}</div>
        </main>
    </div>
</div>


{% block footer %}
    <footer class="footer row">
        Footer LIEN1 LIEN2 LIEN3
    </footer>

{% endblock %}

</body>
</html>
